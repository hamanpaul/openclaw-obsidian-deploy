OPENCLAW_CONFIG_HOST_DIR=/path/to/.openclaw
OPENCLAW_OBSIDIAN_DOCKERFILE=./Dockerfile

OPENCLAW_OBSIDIAN_IMAGE=openclaw-obsidian:local
OPENCLAW_BASE_IMAGE=openclaw:v2026.2.15
OPENCLAW_BASE_IMAGE_DOCKERFILE=/home/paul_chen/ref/code/openclaw/Dockerfile
OPENCLAW_BASE_IMAGE_CONTEXT=/home/paul_chen/ref/code/openclaw
OPENCLAW_DOCKER_APT_PACKAGES=ca-certificates curl jq git ripgrep fd-find yq rsync tzdata python3 python-is-python3
